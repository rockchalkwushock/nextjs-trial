{
  "name": "nextjs-trial",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/rockchalkwushock/nextjs-trial.git",
  "author": "Cody Brunner <rockchalkwushock@icloud.com>",
  "license": "MIT",
  "scripts": {
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "build": "yarn clean && next build",
    "clean": "rm -rf .next out",
    "dev": "next",
    "export": "yarn build && next export",
    "start": "next start",
    "test": "jest --runInBand --updateSnapshot",
    "type-check": "tsc"
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "apollo-boost": "^0.1.19",
    "babel-plugin-react-intl": "^3.0.1",
    "final-form": "^4.10.0",
    "final-form-arrays": "^1.1.0",
    "graphql": "^14.0.2",
    "moment": "^2.22.2",
    "next": "^7.0.2",
    "next-with-apollo": "^3.2.1",
    "react": "^16.6.0",
    "react-apollo": "^2.2.4",
    "react-datepicker": "^1.7.0",
    "react-dom": "^16.6.0",
    "react-final-form": "^3.6.7",
    "react-final-form-arrays": "^1.1.0",
    "react-intl": "^2.7.2",
    "react-portal": "^4.1.5",
    "styled-components": "^4.0.3"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/next": "^7.0.3",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-intl": "^2.3.11",
    "@types/react-portal": "^4.0.1",
    "@types/styled-components": "^4.0.3",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-styled-components": "^1.8.0",
    "dotenv-safe": "^6.1.0",
    "jest": "^23.6.0",
    "jest-dom": "^2.1.0",
    "react-testing-library": "^5.2.3",
    "typescript": "^3.1.4",
    "webpack": "^4.23.1"
  },
  "jest": {
    "bail": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|jsx?|ts?|tsx?)$",
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    }
  }
}
